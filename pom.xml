        <!-- Required Dependency Project -->
        <dependency>
            <groupId>CoreFramework</groupId>
            <artifactId>QA-Commons</artifactId>
            <version>3.0.0-QAA-2060-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>XifinAdminPortalUI</groupId>
            <artifactId>QA-XifinAdminPortalUI</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
        	<groupId>com.xifin.qaautomation.webservices</groupId>
        	<artifactId>qa-restapi-accession</artifactId>
        	<version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.xifin</groupId>
            <artifactId>value-objects</artifactId>
            <version>${value.objects.version}</version>
            <exclusions>
                <exclusion>
                    <groupId>com.fasterxml.jackson.core</groupId>
                    <artifactId>jackson-databind</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>com.xifin</groupId>
                    <artifactId>xifin-persistence</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>com.xifin</groupId>
            <artifactId>xifin-persistence</artifactId>
            <version>${xifin-persistence.version}</version>
        </dependency>
        <dependency>
            <groupId>com.xifin</groupId>
            <artifactId>xifin-encryptionutil</artifactId>
            <version>${xifin-encryptionutil.version}</version>
        </dependency>
        <dependency>
            <groupId>com.xifin</groupId>
            <artifactId>sso-org-configs</artifactId>
            <version>${xifin-sso-org-configs.version}</version>
            <exclusions>
                <exclusion>
                    <groupId>javassist</groupId>
                    <artifactId>javassist</artifactId>
