<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>RpmOverall</groupId>
    <artifactId>QA-RpmOverall</artifactId>
    <version>3.0.0-SNAPSHOT</version>
    <packaging>jar</packaging>

    <name>QA-RpmOverall</name>
    <url>http://maven.apache.org</url>

    <parent>
        <groupId>com.xifin</groupId>
        <artifactId>xifin-common-pom</artifactId>
        <version>5.0.0</version>
    </parent>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <xifin-sso-org-configs.version>15.4.0-105295-A-SNAPSHOT</xifin-sso-org-configs.version>
        <value.objects.version>309.15.03.00.00</value.objects.version>
        <xifin-persistence.version>311.15.06.00.00</xifin-persistence.version>
        <xifin-encryptionutil.version>1.1.0</xifin-encryptionutil.version>
        <selenium.version>3.141.59</selenium.version>
    </properties>

    <distributionManagement>
        <repository>
            <id>xifin</id>
            <name>XIFIN Repo</name>
            <url>${xifin.corporate.repo}</url>
        </repository>
        <snapshotRepository>
            <id>xifinsnapshot</id>
            <name>XIFIN SNAPSHOT Repo</name>
            <url>${xifin.corporate.snapshot.repo}</url>
        </snapshotRepository>
        <site>
            <id>site reports</id>
            <url>${site.reports.url}</url>
        </site>
    </distributionManagement>

    <repositories>
        <repository>
            <id>xifin</id>
            <name>XIFIN Repo</name>
            <releases>
                <checksumPolicy>ignore</checksumPolicy>
            </releases>
            <snapshots>
                <checksumPolicy>ignore</checksumPolicy>
            </snapshots>
            <url>${xifin.corporate.repo}</url>
        </repository>
        <repository>
            <id>xifinsnapshot</id>
            <name>XIFIN Snapshot Repo</name>
            <releases>
                <checksumPolicy>ignore</checksumPolicy>
                <enabled>false</enabled>
            </releases>
            <snapshots>
                <checksumPolicy>ignore</checksumPolicy>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
            </snapshots>
            <url>${xifin.corporate.snapshot.repo}</url>
        </repository>
        <repository>
            <id>com.springsource.repository.bundles.release</id>
            <name>SpringSource Enterprise Bundle Repository - SpringSource Releases</name>
            <url>http://repository.springsource.com/maven/bundles/release</url>
        </repository>
        <repository>
            <id>com.springsource.repository.bundles.external</id>
            <name>SpringSource Enterprise Bundle Repository - External Releases</name>
            <url>http://repository.springsource.com/maven/bundles/external</url>
        </repository>
        <repository>
            <id>jboss.repository.releases.public</id>
            <name>JBoss Repo Public</name>
            <url>http://repository.jboss.org/nexus/content/groups/public-jboss</url>
        </repository>
        <repository>
            <id>smartbear</id>
            <name>smartbear repository</name>
            <url>http://smartbearsoftware.com/repository/maven2</url>
        </repository>
        <repository>
            <id>Enonic</id>
            <name>Enonic</name>
            <url>https://repo.enonic.com/public/</url>
        </repository>
        <repository>
            <id>ICM</id>
            <name>ICM</name>
            <url>http://maven.icm.edu.pl/artifactory/repo/</url>
        </repository>
    </repositories>

